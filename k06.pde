size(400,400);
background(225);

strokeWeight(80);

//blue bottom stroke
stroke(27,46,64,220);
line(220,220,290,300);

//yellow stroke
stroke(255);
line(270,100,120,300);
stroke(255,113,131);
line(120,100,120,300);

//vertocal stroke
stroke(242,183,5,120);
line(270,100,120,300);

noStroke();
fill(255,113,131);
ellipse(120,300,80,80);

save("k06.tif");


